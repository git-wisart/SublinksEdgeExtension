{
    "name": "Sublinks",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to propose various sublinks dependig on the current URL.",
    "icons": {
        "16": "icons/sublinks16x16.png",
        "32": "icons/sublinks32x32.png",
        "48": "icons/sublinks48x48.png",
        "128": "icons/sublinks128x128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "options_page": "options/options.html",
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
    ]
}